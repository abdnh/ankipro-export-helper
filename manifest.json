{
    "manifest_version": 3,
    "name": "AnkiPro Export Helper",
    "description": "Helps you export AnkiPro decks to the real Anki",
    "version": "0.0.1",

    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": ["activeTab", "declarativeContent", "scripting"]
}
